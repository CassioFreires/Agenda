<% if(errors.length > 0) { %>
<div class="row">
    <div class="col">
        <div class="alert alert-danger">
            <% errors.forEach((error) => { %>
                <%= error %>
           <% }); %>
        </div>
    </div>
 </div> 
<% } %>

<% if(success.length > 0) { %>
    <div class="row">
        <div class="col">
            <div class="alert alert-success">
                <% success.forEach((success) => { %>
                    <%= success %>
               <% }); %>
            </div>
        </div>
     </div> 
    <% } %>

